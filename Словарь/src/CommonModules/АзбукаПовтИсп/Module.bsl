Процедура СоответствиеРДСДобавитьДвойныеГласныеСлог(СоответствиеРДС, Наименование)
	
	Часть1 = Лев(Наименование, СтрДлина(Наименование) - 1);
	
	Часть1Моджи = СоответствиеРДС.Получить(НРег(Часть1));
	Если Часть1Моджи = Неопределено Тогда
		ВызватьИсключение СтрШаблон("Моджи %1 нет в соответствии", Часть1);
	КонецЕсли;
	
	Моджи = Новый Массив;
	
	Для Каждого Элемент Из Часть1Моджи Цикл
		Моджи.Добавить(СтруктураДанныхРДС(Элемент.Наименование, Элемент.ЭтоМиниСимвол));
	КонецЦикла;
	
	Часть2 = ВРЕг(Прав(Наименование, 1));
	Если Часть2 = "E" Тогда
		Часть2 = "I"; // ei over ee
	КонецЕсли;
	
	Моджи.Добавить(СтруктураДанныхРДС(Часть2, Ложь));
	
	// СоответствиеРДС.Вставить(НРег(Наименование), Моджи);
	СоответствиеРДС.Вставить(НРег(Часть1) + ":", Моджи);
	
КонецПроцедуры

